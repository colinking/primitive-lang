{
    "name": "primitive-lang",
    "displayName": "Primitive Language Support",
    "description": "Syntax highlighting for Primitive files (.prim)",
    "version": "0.0.1",
    "publisher": "colinking",
    "repository": "https://github.com/colinking/primitive-lang",
    "engines": {
        "vscode": "^1.57.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "primitive",
                "aliases": [
                    "Primitive",
                    "prim"
                ],
                "extensions": [
                    ".prim"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "primitive",
                "scopeName": "source.prim",
                "path": "./syntaxes/primitive.tmLanguage.json"
            }
        ]
    }
}
